# mount point	fstype		device					options
/recovery		emmc		/dev/block/mmcblk0p21
/boot			emmc		/dev/block/mmcblk0p22
/cache			ext4		/dev/block/mmcblk0p28
/data			ext4		/dev/block/mmcblk0p26	length=-16384
/sdcard			vfat		/dev/block/mmcblk1p1	flags=display="SD Card";storage;wipeingui;removable
/system			ext4		/dev/block/mmcblk0p25
/misc			emmc		/dev/block/mmcblk0p17
/devlog			ext4		/dev/block/mmcblk0p29
/emmc			vfat		/dev/block/mmcblk0p32	flags=display="Internal Storage";storage;wipeingui
